<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MakeupArtist">

    <EditText
        android:id="@+id/editTextTextPersonName11"
        android:layout_width="133dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Password:"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="586dp" />

    <EditText
        android:id="@+id/editTextTextPersonName6"
        android:layout_width="79dp"
        android:layout_height="52dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Email:"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="496dp" />

    <EditText
        android:id="@+id/Email"
        android:layout_width="292dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="496dp" />

    <EditText
        android:id="@+id/editTextTextPersonName7"
        android:layout_width="115dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Full Name:"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="371dp" />

    <EditText
        android:id="@+id/Name"
        android:layout_width="253dp"
        android:layout_height="41dp"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="371dp" />

    <EditText
        android:id="@+id/editTextTextPersonName9"
        android:layout_width="148dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Phone Number:"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="430dp" />

    <EditText
        android:id="@+id/Phonenum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Phone number"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="425dp" />

    <EditText
        android:id="@+id/Password"
        android:layout_width="243dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="Password(8 characters only)"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="583dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="189dp"
        android:layout_height="251dp"
        app:srcCompat="@drawable/makeuplogin"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="14dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="259dp"
        android:layout_height="272dp"
        app:srcCompat="@drawable/makeuplogin2"
        tools:layout_editor_absoluteX="190dp"
        tools:layout_editor_absoluteY="14dp" />

    <EditText
        android:id="@+id/editTextTextPersonName12"
        android:layout_width="318dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Registration Form"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="301dp" />

    <Button
        android:id="@+id/addMakeupArtist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="648dp" />
</androidx.constraintlayout.widget.ConstraintLayout>